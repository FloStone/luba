{
    "name": "flo5581/luba",
    "description": "Small PHP Framework",
    "version": "1.2.0",
    "authors": [
        {
            "name": "Flo Stone"
        }
    ],
    "require": {
        "Flo5581/Lubaframework": "1.2.*",
        "Lubaaddons/Exceptionhandler": "1.0.*",
        "Flo5581/mysql": "1.2.*"
    },
    "autoload": {
        "psr-4": {
            "Luba\\Controllers\\": "controllers/",
            "Luba\\Commands\\": "commands/"
        }
    },
    "scripts": {
        "post-install-cmd": [
            "php luba ClearCompiled"
        ],
        "post-update-cmd": [
            "php luba ClearCompiled"
        ]
    }
}
