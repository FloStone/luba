{
    "name": "flostone/luba",
    "description": "Small PHP Framework",
    "version": "2.0.0",
    "authors": [
        {
            "name": "Flo Stone"
        }
    ],
    "require": {
        "FloStone/Lubaframework": "dev-master"
    },
    "autoload": {
        "psr-4": {
            "Luba\\Controllers\\": "controllers/",
            "Luba\\Commands\\": "commands/"
        }
    },
    "scripts": {
        "post-install-cmd": [
            "php luba clear_compiled"
        ],
        "post-update-cmd": [
            "php luba clear_compiled"
        ]
    }
}
